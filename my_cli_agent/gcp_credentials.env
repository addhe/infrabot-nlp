# Simpan file service account key (.json) di direktori yang aman
# Lalu tambahkan path ke file tersebut sebagai environment variable

# Comment out GOOGLE_APPLICATION_CREDENTIALS to use ADC instead of service account
# export GOOGLE_APPLICATION_CREDENTIALS="/Users/addheputra/.config/gcloud/application_default_credentials.json"

# Untuk debugging GCP authentication
export CLOUDSDK_PYTHON_SITEPACKAGES=1

# Simpan API keys yang sudah ada
export GOOGLE_API_KEY="${GOOGLE_API_KEY}"
export OPENAI_API_KEY="${OPENAI_API_KEY}"
export ANTHROPIC_API_KEY="${ANTHROPIC_API_KEY}"
